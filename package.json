{
  "name": "rhs-plus",
  "version": "0.6.5",
  "description": "Provide extra information and automatic actions for Works Applications based pages",
  "author": {
    "name": "danikaze",
    "email": "danikaze@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danikaze/rhs-plus"
  },
  "license": "MIT",
  "engines": {
    "node": "10.15.x",
    "npm": "6.4.x"
  },
  "scripts": {
    "test": "npm run lint",
    "build": "scripts/build.sh",
    "dev": "webpack --config webpack.config.js --env=development",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "clean": "rimraf app",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@types/chrome": "0.0.180",
    "@types/mkdirp": "^0.5.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "husky": "^3.0.5",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.20.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.5.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "vanilla-type-check": "^2.0.3"
  }
}
